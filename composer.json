{
  "name": "mshule/laravel-pipes",
  "description": "A description for laravel-pipes.",
  "type": "package",
  "license": "MIT",
  "keywords": [
    "laravel"
  ],
  "authors": [
    {
      "name": "Krishan Koenig, Otieno Julie",
      "email": "krishan.koenig@googlemail.com"
    }
  ],
  "require": {
    "illuminate/support": "~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
    "illuminate/http": "~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
    "illuminate/routing": "~5.7.0|~5.8.0|^6.0|^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "Mshule\\LaravelPipes\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mshule\\LaravelPipes\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mshule\\LaravelPipes\\LaravelPipesServiceProvider"
      ],
      "aliases": {
        "Pipe": "Mshule\\LaravelPipes\\Facades\\Pipe"
      }
    }
  },
  "require-dev": {
    "orchestra/testbench": "~3.8.0|^4.0|^5.0|^6.0"
  }
}
